{
  "name": "peak-cart",
  "version": "0.0.1",
  "description": "A microservice for powering an e-commerce shopping cart",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "format": "prettier --write jest.config.js e2e/e2e.test.ts 'src/**/*.ts'",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "MODE=e2e jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesseanwright/peak-cart.git"
  },
  "keywords": [
    "microservice",
    "ecommerce",
    "cart",
    "shopping"
  ],
  "author": "James Wright <james@jamesswright.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamesseanwright/peak-cart/issues"
  },
  "homepage": "https://github.com/jamesseanwright/peak-cart#readme",
  "dependencies": {
    "@hapi/hapi": "19.1.1",
    "body-parser": "1.19.0",
    "express": "4.17.1",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@types/body-parser": "1.17.1",
    "@types/express": "^4.17.1",
    "@types/jest": "24.0.18",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "3.4.5",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "typescript": "3.6.3"
  }
}
